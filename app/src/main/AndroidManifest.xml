<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tellabsxcomtelindo.android.klikcoaching">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Klikcoaching">
        <activity android:name=".ui.walkthrou.WalkthruActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.chatdiscussion.DiscussionChatActivity" />
        <activity android:name=".ui.courseplayer.CoursePlayerActivity" />
        <activity
            android:name=".ui.addcomment.AddCommentActivity"
            android:label="@string/title_activity_add_comment"
            android:theme="@style/Theme.Klikcoaching.NoActionBar" />
        <activity android:name=".ui.paymentsuccess.PaymentSuccessfullyActivity" />
        <activity android:name=".ui.pilihpembayaran.ChoosePaymentActivity" />
        <activity android:name=".ui.notifikasi.NotifikasiActivity" />
        <activity android:name=".ui.profile.ProfileActivity" />
        <activity android:name=".ui.coursedetail.CourseDetailActivity" />
        <activity android:name=".ui.detailcategory.DetailCategoryActivity" />
        <activity android:name=".ui.register.RegisterActivity" />
        <activity android:name=".ui.login.LoginActivity">

        </activity>
        <activity android:name=".ui.home.MainActivity" />
    </application>

</manifest>